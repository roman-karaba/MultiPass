{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": ["airbnb"],
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "rules": {
    "no-unused-vars": 1,
    "indent": ["error", 2],
    "strict": [0, "global"],
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "linebreak-style": 0,
    "react/prop-types" : 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-else-return": 0,
    "no-continue": 0
  }
}